# Скопируйте в .env или задайте переменные в системе.
# Файл .env не коммитится в Git.

# База данных (Backend)
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/postgres
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=postgres

# Spring Security — логин и пароль для входа в приложение (user / password по умолчанию)
APP_SECURITY_USER_NAME=user
APP_SECURITY_USER_PASSWORD=password
